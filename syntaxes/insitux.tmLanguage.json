{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Insitux",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#words"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#constants"
    },
    {
      "include": "#comments"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.insitux",
          "match": "(?<=^|[\\[\\(\\{\\s,])(->|#|@|if\\!|if|when|unless|loop-over|loop|match|satisfy|catch|while|function|fn|var\\!|let\\!|var|let|return)(?=[\\]\\)\\}\\s,]|$)"
        },
        {
          "name": "keyword.operation.insitux",
          "match": "(?<=^|[\\[\\(\\{\\s,])(file\\-append|last\\-idx\\-of|lower\\-case|read\\-lines|reductions|skip\\-until|skip\\-while|take\\-until|take\\-while|trim\\-start|upper\\-case|char\\-code|partition|print\\-str|rand\\-pick|skip\\-each|update\\-at|criteria|distinct|flat\\-map|group\\-by|last\\-idx|rand\\-int|rreplace|trim\\-end|average|flatten|letter\\?|part\\-by|prepend|replace|reverse|shuffle|sort\\-by|starts\\?|substr\\?|symbols|type\\-of|version|append|digit\\?|empty\\?|fast\\!\\=|fast\\/\\/|fast\\<\\=|fast\\>\\=|filter|idx\\-of|import|insert|lower\\?|prompt|reduce|remove|repeat|sample|set\\-at|space\\?|to\\-key|to\\-num|to\\-vec|toggle|upper\\?|about|assoc|bool\\?|clamp|count|dict\\?|ends\\?|even\\?|fast\\*|fast\\+|fast\\-|fast\\/|fast\\<|fast\\=|fast\\>|first|floor|freqs|func\\?|log10|null\\?|print|punc\\?|range|recur|reset|round|sieve|split|str\\<\\=|str\\>\\=|tests|times|trunc|wild\\?|write|zero\\?|acos|all\\?|args|asin|atan|bool|ceil|comp|cosh|crop|dict|div\\?|drop|eval|exec|ext\\?|find|into|join|juxt|key\\?|keys|last|log2|logn|neg\\?|num\\?|odd\\?|omit|pos\\?|rand|read|sect|sinh|skip|sort|sqrt|str\\*|str\\<|str\\>|str\\?|strn|tanh|time|trim|vals|vec\\?|xmap|\\.\\.\\.|\\>\\>\\>|abs|adj|and|cos|dec|for|idx|inc|len|map|max|min|neg|rem|sin|str|tan|val|vec|xor|\\!\\=|\\*\\*|\\.\\.|\\/\\/|\\<\\<|\\<\\=|\\=\\=|\\>\\=|\\>\\>|PI|do|or|\\!|&|\\*|\\+|\\-|\\.|\\/|\\<|\\=|\\>|E|\\^|\\||~)(?=[\\]\\)\\}\\s,|$])"
        }
      ]
    },
    "words": {
      "patterns": [
        {
          "name": "variable.language",
          "match": "(?<=^|[\\[\\(\\{\\s,])(\\$.+?)(?=[\\]\\)\\}\\s,]|$)"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.insitux",
      "patterns": [
        {
          "name": "string.quoted.single.insitux",
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "name": "constant.character.escape.insitux",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.double.insitux",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.insitux",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "constants": {
      "patterns": [
        {
          "name": "constant.numeric.insitux",
          "match": "(?<=^|[\\[\\(\\{\\s,])(%\\d*|\\d+|true|false|args|(:[^\\]\\)\\}]+?))(?=[\\]\\)\\}\\s,]|$)"
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "name": "comment.line",
          "begin": ";",
          "end": "$"
        }
      ]
    }
  },
  "scopeName": "source.insitux"
}
