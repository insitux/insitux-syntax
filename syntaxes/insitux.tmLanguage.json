{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Insitux",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#words"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#constants"
    },
    {
      "include": "#comments"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.insitux",
          "match": "(?<=^|[\\[\\(\\{\\s])(#|@|if\\!|if|when|unless|loop|match|catch|while|function|fn|var\\!|let\\!|var|let|return)(?=[\\]\\)\\}\\s]|$)"
        },
        {
          "name": "keyword.operation.insitux",
          "match": "(?<=^|[\\[\\(\\{\\s])(lower\\-case|read\\-lines|trim\\-start|upper\\-case|char\\-code|print\\-str|rand\\-int|trim\\-end|distinct|reverse|sort\\-by|starts\\?|substr\\?|symbolsversion|prepend|append|empty\\?|fast\\!\\=|fast\\/\\/|fast\\<\\=|fast\\>\\=|filter|reduce|remove|insert|repeat|set\\-at|to\\-key|to\\-num|bool\\?|count|dict\\?|ends\\?|even\\?|fast\\*|fast\\+|fast\\-|fast\\/|fast\\<|fast\\=|fast\\>|floor|func\\?|log10|null\\?|print|range|recur|reset|round|split|tests|wild\\?|zero\\?|omit|acos|asin|atan|ceil|cosh|dict|eval|ext\\?|find|into|join|key\\?|keys|log2|logn|neg\\?|num\\?|odd\\?|pos\\?|rand|sect|sinh|sort|sqrt|str\\*|str\\?|tanh|time|trim|vals|vec\\?|\\.\\.\\.|\\>\\>\\>|abs|and|cos|dec|for|idx|inc|len|map|max|min|rem|sin|str|tan|val|vec|xor|\\!\\=|\\*\\*|\\-\\>|\\.\\.|\\/\\/|\\<\\<|\\<\\=|\\>\\=|\\>\\>|PI|do|or|\\!|&|\\*|\\+|\\-|\\.|\\/|\\<|\\=|\\>|E|\\^|\\||~)(?=[\\]\\)\\}\\s|$])"
        }
      ]
    },
    "words": {
      "patterns": [
        {
          "name": "variable.language",
          "match": "(?<=^|[\\[\\(\\{\\s])(\\$.+?)(?=[\\]\\)\\}\\s]|$)"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.insitux",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.insitux",
          "match": "\\\\."
        }
      ]
    },
    "constants": {
      "patterns": [
        {
          "name": "constant.numeric.insitux",
          "match": "(?<=^|[\\[\\(\\{\\s])(%\\d*|\\d+|true|false|args|(:[^\\]\\)\\}]+?))(?=[\\]\\)\\}\\s]|$)"
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "name": "comment.line",
          "begin": ";",
          "end": "$"
        }
      ]
    }
  },
  "scopeName": "source.insitux"
}
